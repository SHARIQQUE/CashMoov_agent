<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="350dp"
    android:layout_height="475dp"
    android:layout_gravity="center">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        android:background="@color/colorPrimary">

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_centerInParent="true"
            android:background="@drawable/check" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="16dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20dp"
            android:text="Service de consultation des frais Cash Moov"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
            android:gravity="center_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Service choisi:"
            android:textSize="18dp"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
            android:gravity="center_horizontal" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Retrait"
            android:textColor="@color/green_700"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
            android:gravity="center_horizontal" />


        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                app:srcCompat="@drawable/ic_baseline_check_circle_outline_24"
                />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="De 10 000 à 50 000 :"
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />

            <TextView
                android:id="@+id/sprovider"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1000 GNF "
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:layout_marginLeft="10dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />


        </LinearLayout>

        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                app:srcCompat="@drawable/ic_baseline_check_circle_outline_24"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="De 50 001 à 100 000:"
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />

            <TextView
                android:id="@+id/mobile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5000 GNF"
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:layout_marginLeft="10dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />


        </LinearLayout>
        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="16dp"
                android:layout_height="wrap_content"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                app:srcCompat="@drawable/ic_baseline_check_circle_outline_24"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="De 100 001 à 300 000:"
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />

            <TextView
                android:id="@+id/mobil"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="8000 GNF"
                android:layout_gravity="clip_horizontal"
                android:paddingTop="5dp"
                android:layout_marginLeft="10dp"
                android:paddingBottom="5dp"
                android:textColor="#000"
                />


        </LinearLayout>
        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"/>

        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"/>

        <LinearLayout
            android:gravity="left"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"/>

        <LinearLayout
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/buttonanule"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:background="@drawable/btn_blue"
                android:text="Fermer"
                android:textColor="@color/white" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>