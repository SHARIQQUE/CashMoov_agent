<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".wallet_owner.agent.AgentKYC">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/_15sdp"
        android:orientation="vertical">



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="@dimen/_6sdp"
                app:cardElevation="0dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingStart="@dimen/_15sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:paddingBottom="@dimen/_15sdp">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="@dimen/_15sdp"
                        android:src="@drawable/profil" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="25sp"
                        android:textColor="#29304d"
                        android:fontFamily="@font/aileron_bold"
                        android:lineSpacingExtra="7sp"
                        android:text="@string/enter_info"
                        android:layout_gravity="center"
                        />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="17sp"
                        android:textColor="#323232"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:fontFamily="@font/aileron_semibold"
                        android:lineSpacingExtra="8sp"
                        android:textAlignment="center"
                        android:text="@string/user_info_msg"
                        android:gravity="center_horizontal" />


                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/_10sdp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spAccType"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/agent"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"  />
                </LinearLayout>


                <EditText
                    android:id="@+id/etAgentName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="text"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_agent_name"
                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />


                <EditText
                    android:id="@+id/etLname"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="text"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_lname"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />

                <EditText
                    android:id="@+id/etEmail"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="textEmailAddress"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_email"
                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />

                <EditText
                    android:id="@+id/etPhone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:drawableRight="@drawable/ic_contact"
                    android:drawablePadding="@dimen/_10sdp"
                    android:inputType="phone"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_phone_number"
                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spBusinessType"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_business_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"  />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spCountry"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_country"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"  />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spRegion"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_region"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"  />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spCity"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_city"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                </LinearLayout>
                <EditText
                    android:id="@+id/etAddress"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="text"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_address"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spGender"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_gender"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                </LinearLayout>
                <TextView
                    android:id="@+id/dobText"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginStart="10dp"
                    android:textStyle="bold"
                    android:paddingTop="10dp"
                    android:visibility="gone"
                    android:textSize="@dimen/_15sdp"

                    android:text="Date Of Birth*"></TextView>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">


                    <EditText
                        android:id="@+id/etDob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:layout_weight="1"

                        android:lineSpacingExtra="10sp"
                        android:paddingLeft="@dimen/_15sdp"
                        android:paddingTop="@dimen/_12sdp"
                        android:background="@null"
                        android:focusable="false"


                        android:paddingBottom="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_15sdp"
                        android:hint="@string/valid_dob"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                    <ImageView
                        android:id="@+id/calenderIcon_Image"
                        android:src="@drawable/cal"
                        android:layout_width="30dp"
                        android:layout_gravity="center"
                        android:paddingRight="10dp"
                        android:layout_height="30dp" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spIdProof"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/valid_select_id_proof"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"  />
                </LinearLayout>

                <EditText
                    android:id="@+id/etProofNo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="phone"
                    android:digits="0123456-/"                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/vaild_id_proof_no"
                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="horizontal"
                    android:padding="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_10sdp">

                        <com.suke.widget.SwitchButton
                            android:id="@+id/sbLoginwithotp"
                            android:layout_width="wrap_content"
                            app:sb_checked="false"
                            android:layout_height="wrap_content" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:lineSpacingExtra="4sp"
                        android:text="@string/login_with_otp"
                        android:layout_gravity="center"
                        android:textColor="#000"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                     </LinearLayout>

                <TextView
                    android:id="@+id/tvNext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_13sdp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="3sp"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:background="@drawable/btn_blue"
                    android:layout_gravity="center"
                    android:paddingTop="@dimen/_15sdp"
                    android:paddingBottom="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_bold"
                    android:gravity="center"
                    android:text="@string/next" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>
</androidx.core.widget.NestedScrollView>