<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/click_row_layout"
    android:orientation="horizontal">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"



        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"


            android:orientation="horizontal"
            android:weightSum="2">


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:weightSum="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/agentName_textview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="@dimen/_5sdp"

                    android:gravity="left"
                    android:padding="2dp"
                    android:textColor="@color/textColorBlack"
                    android:textSize="@dimen/_13sdp"
                    android:text="@string/agentName"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/email_textview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:textStyle="bold"
                    android:textSize="@dimen/_10sdp"

                    android:gravity="left"
                    android:padding="2dp"
                    android:text="@string/email"
                    android:textColor="@color/black"
                    />






            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="1"
                android:padding="@dimen/_10sdp"

                android:orientation="vertical">

                <TextView
                    android:id="@+id/agent_mobileNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:gravity="left"
                    android:padding="2dp"

                    android:text="@string/agent_mobileNumber"
                    android:textColor="@color/red_600"
                    android:textSize="12sp"


                    android:textStyle="bold"></TextView>


                <TextView
                    android:id="@+id/countryName_textview"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:gravity="left"
                    android:textStyle="bold"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="@color/green_600"

                    android:padding="2dp"
                    android:text="@string/country"
                    ></TextView>





            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="3"
            android:padding="@dimen/_5sdp"
            android:background="@drawable/round_shape"

            android:orientation="horizontal" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_weight="0.1"
                android:layout_gravity="center"

                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="@dimen/_25sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:textColor="#ffffff"
                    android:fontFamily="@font/aileron_bold"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:lineSpacingExtra="10sp"
                    app:srcCompat="@drawable/ic_lewallet" />

                />
            </LinearLayout>

            <LinearLayout
                android:layout_width="175dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_weight="1"

                android:orientation="vertical">

                <TextView
                    android:id="@+id/transType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/source"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/source"
                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/destination"
                    android:textColor="@color/textcolorheading"

                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/source_msisdn"
                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"

                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/dest_msisdn"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/recharge_no"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/amounts"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"

                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/fee"
                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"

                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/tax_type"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/tax"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/trans_reversed"
                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"

                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/trans_date"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="@string/status"

                    android:textColor="@color/textcolorheading"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold"
                    android:visibility="gone" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"


                android:orientation="vertical">

                <TextView
                    android:id="@+id/amount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/transDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />


                <TextView
                    android:id="@+id/transId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:visibility="gone"

                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />




                <TextView
                    android:id="@+id/destination"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/sourceMSISDN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/destMSISDN"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:visibility="gone"

                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />


                <TextView
                    android:id="@+id/rechargeNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />



                <TextView
                    android:id="@+id/fee"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/taxType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:visibility="gone"

                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/tax"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />

                <TextView
                    android:id="@+id/transReversed"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:text="N/A"
                    android:visibility="gone"

                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />



                <TextView
                    android:id="@+id/status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5"
                    android:visibility="gone"

                    android:text="N/A"
                    android:textColor="@color/textcolor"
                    android:textSize="@dimen/_11sdp" />
            </LinearLayout>




        </LinearLayout>



    </LinearLayout>





</LinearLayout>