<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".servicecharge.ServiceCharge">

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="vertical">

    <include layout="@layout/toolbar_active" />

 <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginStart="@dimen/_15sdp"
        android:layout_marginEnd="@dimen/_15sdp"
        android:layout_marginBottom="@dimen/_15sdp">

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aileron_bold"
        android:lineSpacingExtra="3sp"
        android:text="@string/service_charge"
        android:textColor="#29304d"
        android:textSize="@dimen/_16sdp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1" />

  </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_25sdp"
        android:orientation="horizontal">

      <androidx.cardview.widget.CardView
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          app:cardCornerRadius="@dimen/_6sdp"
          app:cardBackgroundColor="@color/bleu_cashmoov">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/_10sdp">

          <FrameLayout
              android:layout_width="wrap_content"
              android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:textColor="#ffffff"
                android:fontFamily="@font/aileron_bold"
                android:layout_gravity="center"
                android:gravity="center"
                android:lineSpacingExtra="10sp"
                app:srcCompat="@drawable/logo200x70"

                />
          </FrameLayout>
        </LinearLayout>
      </androidx.cardview.widget.CardView>

    </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="#F9F9F9"
      android:orientation="vertical"
      android:layout_marginTop="@dimen/_20sdp">

    <LinearLayout
        android:id="@+id/linRemittance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_transfer"
          app:tint="@color/colorPrimary"/>


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical"
          android:layout_gravity="center_vertical">

        <TextView
            android:id="@+id/tvRemittance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/remittance"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />


        <TextView
            android:id="@+id/tvFeeRemittance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_2sdp"
            android:fontFamily="@font/aileron_bold"
            android:gravity="right"
            android:lineSpacingExtra="10sp"
            android:text="@string/free_service"
            android:textColor="@color/bleu_cashmoov"
            android:textSize="@dimen/_11sdp" />

      </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linCreditPurchase"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_airtime"
          app:tint="@color/colorPrimary"/>

      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical"
          android:layout_gravity="center_vertical">

        <TextView
            android:id="@+id/tvCreditPurchase"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/credit_purchase"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />

          <TextView
              android:id="@+id/tvFeeCreditPurchase"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/aileron_bold"
              android:gravity="right"
              android:lineSpacingExtra="10sp"
              android:text="@string/free_service"
              android:textColor="@color/bleu_cashmoov"
              android:textSize="@dimen/_11sdp"
              android:layout_marginTop="@dimen/_2sdp"/>

        </LinearLayout>
      </LinearLayout>

    <LinearLayout
        android:id="@+id/linBillPay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_baseline_payment_24" />


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical"
          android:layout_gravity="center_vertical">

        <TextView
            android:id="@+id/tvBillPayment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/bill_payment"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />

        <TextView
            android:id="@+id/tvFeeBillPayment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="right"
            android:lineSpacingExtra="10sp"
            android:text="@string/free_service"
            android:textColor="@color/bleu_cashmoov"
            android:textSize="@dimen/_11sdp"
            android:layout_marginTop="@dimen/_2sdp"/>

      </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linMoneyTransfer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_paymane24" />


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:orientation="vertical"
          android:layout_marginLeft="@dimen/_10sdp"
          android:layout_gravity="center_vertical">

        <TextView
            android:id="@+id/tvMoneyTransfer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/money_transfer"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />

          <TextView
              android:id="@+id/tvFeeMoneyTransfer"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/aileron_bold"
              android:gravity="right"
              android:lineSpacingExtra="10sp"
              android:text="@string/free_service"
              android:textColor="@color/bleu_cashmoov"
              android:textSize="@dimen/_11sdp"
              android:layout_marginTop="@dimen/_2sdp"/>

        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linCashIn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:padding="@dimen/_10sdp">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_cashin" />


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical">

        <TextView
            android:id="@+id/tvCashIn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/cash_In"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />

          <TextView
              android:id="@+id/tvFeeCashIn"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/aileron_bold"
              android:gravity="right"
              android:lineSpacingExtra="10sp"
              android:text="@string/paid_service"
              android:textColor="@color/bleu_cashmoov"
              android:textSize="@dimen/_11sdp"
              android:layout_marginTop="@dimen/_2sdp"/>

        </LinearLayout>
      </LinearLayout>


    <LinearLayout
        android:id="@+id/linCashOut"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          app:srcCompat="@drawable/ic_cashout"
          />


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical"
          android:layout_gravity="center_vertical">

        <TextView
            android:id="@+id/tvCashOut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_bold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/cash_Out"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />


          <TextView
              android:id="@+id/tvFeeCashOut"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/aileron_bold"
              android:gravity="right"
              android:lineSpacingExtra="10sp"
              android:text="@string/paid_service"
              android:textColor="@color/bleu_cashmoov"
              android:textSize="@dimen/_11sdp"
              android:layout_marginTop="@dimen/_2sdp"/>

        </LinearLayout>
      </LinearLayout>


    <LinearLayout
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10sdp"
        android:background="#ffffff"
        android:orientation="horizontal">

      <ImageView
          android:layout_width="@dimen/_35sdp"
          android:layout_height="@dimen/_35sdp"
          android:src="@drawable/ic_baseline_cashin" />


      <LinearLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="@dimen/_10sdp"
          android:orientation="vertical"
          android:layout_gravity="center_vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/aileron_semibold"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="10sp"
            android:text="@string/money_transfer"
            android:textColor="#3a3a3a"
            android:textSize="@dimen/_13sdp" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/aileron_semibold"
              android:gravity="right"
              android:lineSpacingExtra="10sp"
              android:text="621 78 87 89"
              android:textColor="@color/bleu_cashmoov"
              android:textSize="@dimen/_11sdp"
              android:layout_marginTop="@dimen/_2sdp"/>

        </LinearLayout>
      </LinearLayout>



  </LinearLayout>
  </LinearLayout>


</LinearLayout>
</androidx.core.widget.NestedScrollView>