<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <include layout="@layout/toolbar_active"></include>

            <LinearLayout
                android:id="@+id/ll_sender_part_2"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginStart="@dimen/_15sdp"
                android:layout_marginEnd="@dimen/_15sdp"
                android:layout_marginBottom="@dimen/_15sdp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/aileron_bold"
                        android:lineSpacingExtra="3sp"
                        android:text="@string/receive_money"
                        android:textColor="#29304d"
                        android:textSize="@dimen/_16sdp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_14sdp"
                    android:textColor="#29304d"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:fontFamily="@font/aileron_bold"
                    android:lineSpacingExtra="3sp"
                    android:text="@string/outlet_details" />




                <EditText
                    android:id="@+id/et_sender_merchantName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="textPersonName"
                    android:lineSpacingExtra="10sp"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/outlet_name_star"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />





                <TextView
                    android:id="@+id/dobText"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginStart="10dp"
                    android:textStyle="bold"
                    android:paddingTop="10dp"
                    android:visibility="gone"
                    android:textSize="@dimen/_15sdp"

                    android:text="@string/valid_dob"></TextView>

                <EditText
                    android:id="@+id/et_sender_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:visibility="gone"
                    android:inputType="textPersonName"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_address"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />

                <TextView
                    android:id="@+id/spinner_senderCountry"
                    android:text="@string/sending_country_star"
                    android:visibility="gone"
                    style="@style/SpinnerStyleNew1"/>

                <TextView
                    android:id="@+id/spinner_sender_region"
                    android:text="@string/valid_select_region"
                    android:visibility="gone"
                    style="@style/SpinnerStyleNew1" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="visible"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="phone"
                    android:lineSpacingExtra="10sp"
                    android:digits="0123456789-/"

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/surname_star"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />


                <EditText
                    android:id="@+id/et_sender_idproofNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="visible"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="phone"
                    android:lineSpacingExtra="10sp"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/email_star"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />

                <TextView
                    android:id="@+id/dobidproffText"
                    android:layout_width="match_parent"
                    android:layout_height="30dp"
                    android:layout_marginStart="10dp"
                    android:textStyle="bold"
                    android:paddingTop="10dp"
                    android:visibility="gone"
                    android:textSize="@dimen/_15sdp"

                    android:text="@string/vaild_id_proof_no"></TextView>





                <TextView
                    android:text="@string/operatingCountry"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>
                <TextView
                    android:text="@string/business_type_reveiewPage"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="textPersonName"
                    android:lineSpacingExtra="10sp"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/business"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />
                <AutoCompleteTextView
                    android:id="@+id/et_sender_phoneNumber"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="number"
                    android:lineSpacingExtra="10sp"
                    android:drawableRight="@drawable/ic_contact"
                    android:drawablePadding="@dimen/_10sdp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/valid_phone_number"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingTop="@dimen/_7sdp"
                    android:paddingBottom="@dimen/_7sdp"
                    android:paddingEnd="@dimen/_10sdp"
                    android:textColor="@color/textColorLBlack">
                    <TextView
                        android:id="@+id/spIdProof"
                        android:padding="@dimen/_5sdp"
                        android:drawablePadding="@dimen/_5sdp"
                        android:drawableRight="@drawable/ic_arrow_down"
                        android:text="@string/id_proof_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold"/>
                </LinearLayout>
                <EditText
                    android:id="@+id/etProofNo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:inputType="text"
                    android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890/-"
                    android:lineSpacingExtra="10sp"
                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/vaild_id_proof_no"
                    android:textColor="@color/textColorLBlack"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:layout_marginTop="10dp"
                    android:visibility="visible"
                    android:orientation="horizontal">


                    <EditText
                        android:id="@+id/et_sender_idproof_expiry"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:layout_weight="1"

                        android:lineSpacingExtra="10sp"
                        android:paddingLeft="@dimen/_15sdp"
                        android:paddingTop="@dimen/_12sdp"
                        android:background="@null"
                        android:focusable="false"


                        android:paddingBottom="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_15sdp"
                        android:hint="@string/valid_id_proof_expiryDate"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                    <ImageView
                        android:id="@+id/calenderIconidproff_Image"
                        android:src="@drawable/cal"
                        android:layout_width="30dp"
                        android:layout_gravity="center"
                        android:paddingRight="10dp"
                        android:layout_height="30dp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/spinner_sender_issuingCountry"
                    android:text="@string/valid_select_issuing_country"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="@color/bgColorHint"
                    android:layout_marginTop="10dp"
                    android:visibility="visible"
                    android:orientation="horizontal">


                    <EditText
                        android:id="@+id/et_sender_dateofbirth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:layout_weight="1"

                        android:lineSpacingExtra="10sp"
                        android:paddingLeft="@dimen/_15sdp"
                        android:paddingTop="@dimen/_12sdp"
                        android:background="@null"
                        android:focusable="false"


                        android:paddingBottom="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_15sdp"
                        android:hint="@string/Date_OF_Birth_star"
                        android:textColor="@color/textColorLBlack"
                        android:textSize="@dimen/_15sdp"
                        android:fontFamily="@font/aileron_semibold" />
                    <ImageView
                        android:id="@+id/calenderIcondob_Image"
                        android:src="@drawable/cal"
                        android:layout_width="30dp"
                        android:layout_gravity="center"
                        android:paddingRight="10dp"
                        android:layout_height="30dp" />
                </LinearLayout>
                <TextView
                    android:id="@+id/spinner_sender_gender"
                    android:text="@string/valid_select_gender"
                    style="@style/SpinnerStyleNew1" />

                <TextView
                    android:text="@string/Address_Type_star"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>

                <EditText
                    android:id="@+id/et_sender_address1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="visible"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="phone"
                    android:lineSpacingExtra="10sp"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/address_star"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />
                <EditText
                    android:id="@+id/et_address"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:visibility="visible"
                    android:background="@drawable/btn_edittextname"
                    android:backgroundTint="#999999"
                    android:inputType="phone"
                    android:lineSpacingExtra="10sp"
                    android:digits="abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ "

                    android:paddingLeft="@dimen/_15sdp"
                    android:paddingTop="@dimen/_12sdp"
                    android:paddingBottom="@dimen/_12sdp"
                    android:paddingEnd="@dimen/_15sdp"
                    android:hint="@string/address"
                    android:textColor="#323232"
                    android:textSize="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_semibold"  />
                <TextView
                    android:text="@string/country_star"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>

                <TextView
                    android:text="@string/region_name_star"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>

                <TextView
                    android:text="@string/city_star"
                    android:visibility="visible"
                    style="@style/SpinnerStyleNew1"/>

                <TextView
                    android:id="@+id/tvNext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_13sdp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="3sp"
                    android:layout_marginTop="@dimen/_30sdp"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:background="@drawable/btn_blue"
                    android:layout_gravity="center"
                    android:paddingTop="@dimen/_15sdp"
                    android:paddingBottom="@dimen/_15sdp"
                    android:fontFamily="@font/aileron_bold"
                    android:gravity="center"
                    android:text="@string/next" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
